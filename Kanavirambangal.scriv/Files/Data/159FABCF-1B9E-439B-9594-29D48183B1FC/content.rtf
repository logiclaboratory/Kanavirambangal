\par\plain {\f0\fs24\b0 \loch\af0\hich\af0\dbch\af0\uc1\u3351\'3F\u3403\'3F\u3370\'3F\u3390\'3F\u3378\'3F\u3391\'3F\u3368\'3F\u3405\'3F\u8204\'3F \u3354\'3F\u3390\'3F\u3375\'3F\u3375\'3F\u3393\'3F\u3359\'3F\u3398\'3F \u3349\'3F\u3390\'3F\u3382\'3F\u3405\'3F \u3349\'3F\u3402\'3F\u3359\'3F\u3393\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3393\'3F \u3370\'3F\u3393\'3F\u3377\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3399\'3F\u3375\'3F\u3405\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3391\'3F\u3377\'3F\u3353\'3F\u3405\'3F\u3353\'3F\u3391\'3F\u3375\'3F\u3370\'3F\u3405\'3F\u3370\'3F\u3403\'3F\u3454\'3F \u3374\'3F\u3380\'3F\u3375\'3F\u3405\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3405\'3F \u3370\'3F\u3405\'3F\u3376\'3F\u3363\'3F\u3375\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3391\'3F\u3368\'3F\u3405\'3F\u8205\'3F\u3377\'3F\u3398\'3F \u3373\'3F\u3390\'3F\u3381\'3F\u3374\'3F\u3390\'3F\u3375\'3F\u3391\'3F\u3376\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3393\'3F. \u3374\'3F\u3393\'3F\u3350\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3393\'3F \u3381\'3F\u3392\'3F\u3380\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3347\'3F\u3376\'3F\u3403\'3F \u3374\'3F\u3380\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3393\'3F\u3379\'3F\u3405\'3F\u3379\'3F\u3391\'3F\u3375\'3F\u3393\'3F\u3330\'3F \u3335\'3F\u3384\'3F\u3375\'3F\u3393\'3F\u3359\'3F\u3398\'3F \u3354\'3F\u3393\'3F\u3330\'3F\u3372\'3F\u3368\'3F\u3353\'3F\u3405\'3F\u3353\'3F\u3454\'3F \u3370\'3F\u3403\'3F\u3378\'3F\u3398\'3F... \u3333\'3F\u3381\'3F \u3381\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3393\'3F \u3370\'3F\u3364\'3F\u3391\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3364\'3F\u3405\'3F \u3342\'3F\u3368\'3F\u3405\'3F\u8205\'3F\u3377\'3F\u3398\'3F \u3334\'3F\u3364\'3F\u3405\'3F\u3374\'3F\u3390\'3F\u3381\'3F\u3391\'3F\u3378\'3F\u3390\'3F\u3363\'3F\u3398\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3393\'3F \u3364\'3F\u3403\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3391\'3F.}
\par\plain {\f0\fs24\b0 \loch\af0\hich\af0\dbch\af0\uc1\u3374\'3F\u3393\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3390\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3391\'3F \u3375\'3F\u3390\'3F\u3364\'3F\u3405\'3F\u3376\'3F\u3375\'3F\u3405\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F \u3333\'3F\u3381\'3F\u3367\'3F\u3391\'3F \u3349\'3F\u3402\'3F\u3359\'3F\u3393\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3393\'3F\u3349\'3F\u3402\'3F\u3363\'3F\u3405\'3F\u3359\'3F\u3405\'3F \u3381\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3381\'3F\u3380\'3F\u3391\'3F\u3375\'3F\u3391\'3F\u3378\'3F\u3399\'3F\u3375\'3F\u3405\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3405\'3F \u3372\'3F\u3400\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3405\'3F \u3364\'3F\u3391\'3F\u3376\'3F\u3391\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F\u3374\'3F\u3405\'3F\u3370\'3F\u3403\'3F\u3454\'3F \u3342\'3F\u3364\'3F\u3405\'3F\u3376\'3F\u3375\'3F\u3393\'3F\u3330\'3F \u3381\'3F\u3399\'3F\u3351\'3F\u3330\'3F \u3335\'3F\u3384\'3F\u3375\'3F\u3393\'3F\u3359\'3F\u3398\'3F \u3333\'3F\u3359\'3F\u3393\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3398\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3390\'3F\u3451\'3F \u3374\'3F\u3368\'3F\u3384\'3F\u3405\'3F\u3384\'3F\u3405\'3F \u3381\'3F\u3398\'3F\u3374\'3F\u3405\'3F\u3370\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3393\'3F\u3363\'3F\u3405\'3F\u3359\'3F\u3390\'3F\u3375\'3F\u3391\'3F\u3376\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3393\'3F. \u3370\'3F\u3402\'3F\u3364\'3F\u3393\'3F\u3381\'3F\u3398\'3F \u3374\'3F\u3359\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3375\'3F\u3390\'3F\u3364\'3F\u3405\'3F\u3376\'3F\u3349\'3F\u3454\'3F \u3370\'3F\u3398\'3F\u3359\'3F\u3405\'3F\u3359\'3F\u3398\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3381\'3F\u3384\'3F\u3390\'3F\u3368\'3F\u3391\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3390\'3F\u3377\'3F\u3393\'3F\u3363\'3F\u3405\'3F\u3359\'3F\u3405\'3F. \u3349\'3F\u3363\'3F\u3405\'3F\u3359\'3F\u3393\'3F \u3364\'3F\u3392\'3F\u3452\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3349\'3F\u3390\'3F\u3380\'3F\u3405\'3F\u3354\'3F\u3349\'3F\u3379\'3F\u3393\'3F\u3359\'3F\u3398\'3F \u3370\'3F\u3393\'3F\u3364\'3F\u3393\'3F\u3374\'3F \u3354\'3F\u3403\'3F\u3452\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3364\'3F\u3391\'3F\u3376\'3F\u3391\'3F\u3354\'3F\u3405\'3F\u3354\'3F\u3393\'3F \u3381\'3F\u3376\'3F\u3381\'3F\u3393\'3F\u3349\'3F\u3379\'3F\u3390\'3F\u3363\'3F\u3405\'3F \u3347\'3F\u3376\'3F\u3403\'3F \u3374\'3F\u3359\'3F\u3349\'3F\u3405\'3F\u3349\'3F \u3375\'3F\u3390\'3F\u3364\'3F\u3405\'3F\u3376\'3F\u3375\'3F\u3393\'3F\u3330\'3F. \u3333\'3F\u3364\'3F\u3405\'3F \u3349\'3F\u3402\'3F\u3363\'3F\u3405\'3F\u3359\'3F\u3405\'3F \u3364\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3398\'3F \u3381\'3F\u3391\'3F\u3376\'3F\u3384\'3F\u3381\'3F\u3393\'3F\u3330\'3F. \u3335\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3398\'3F \u3374\'3F\u3359\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3375\'3F\u3390\'3F\u3364\'3F\u3405\'3F\u3376\'3F \u3346\'3F\u3376\'3F\u3391\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3378\'3F\u3393\'3F\u3330\'3F \u3374\'3F\u3359\'3F\u3393\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3390\'3F\u3364\'3F\u3405\'3F\u3364\'3F \u3346\'3F\u3376\'3F\u3393\'3F \u3349\'3F\u3390\'3F\u3378\'3F\u3352\'3F\u3359\'3F\u3405\'3F\u3359\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3391\'3F\u3378\'3F\u3399\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F\u3379\'3F\u3405\'3F\u3379\'3F \u3374\'3F\u3359\'3F\u3353\'3F\u3405\'3F\u3353\'3F\u3391\'3F \u3370\'3F\u3403\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3390\'3F\u3363\'3F\u3405\'3F. \u3335\'3F\u3384\'3F\u3375\'3F\u3393\'3F\u3374\'3F\u3402\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3393\'3F\u3379\'3F\u3405\'3F\u3379\'3F \u3349\'3F\u3380\'3F\u3391\'3F\u3358\'3F\u3405\'3F\u3358\'3F \u3349\'3F\u3390\'3F\u3378\'3F \u3356\'3F\u3392\'3F\u3381\'3F\u3391\'3F\u3364\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3391\'3F\u3378\'3F\u3399\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F\u3379\'3F\u3405\'3F\u3379\'3F \u3374\'3F\u3359\'3F\u3349\'3F\u3405\'3F\u3349\'3F \u3375\'3F\u3390\'3F\u3364\'3F\u3405\'3F\u3376\'3F.}
\par\plain {\f0\fs24\b0 \loch\af0\hich\af0\dbch\af0\uc1\u3364\'3F\u3393\'3F\u3379\'3F\u3405\'3F\u3379\'3F\u3391\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3402\'3F\u3376\'3F\u3393\'3F \u3349\'3F\u3393\'3F\u3359\'3F\u3330\'3F \u3370\'3F\u3398\'3F\u3375\'3F\u3405\'3F\u3375\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3374\'3F\u3380\'3F\u3375\'3F\u3393\'3F\u3330\'3F, \u3368\'3F\u3368\'3F\u3358\'3F\u3405\'3F\u3358\'3F\u3393\'3F \u3364\'3F\u3398\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3391\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3398\'3F\u3377\'3F\u3391\'3F\u3354\'3F\u3405\'3F\u3354\'3F\u3393\'3F \u3349\'3F\u3391\'3F\u3359\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3349\'3F\u3368\'3F\u3349\'3F\u3405\'3F\u3370\'3F\u3393\'3F\u3376\'3F \u3377\'3F\u3403\'3F\u3361\'3F\u3393\'3F\u3330\'3F, \u3335\'3F\u3359\'3F\u3375\'3F\u3405\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3391\'3F\u3359\'3F\u3398\'3F \u3370\'3F\u3405\'3F\u3376\'3F\u3364\'3F\u3405\'3F\u3375\'3F\u3349\'3F\u3405\'3F\u3383\'3F\u3370\'3F\u3405\'3F\u3370\'3F\u3398\'3F\u3359\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3381\'3F\u3390\'3F\u3376\'3F\u3391\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F\u3380\'3F\u3391\'3F\u3349\'3F\u3379\'3F\u3393\'3F\u3330\'3F \u3349\'3F\u3359\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3393\'3F \u3374\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3403\'3F\u3359\'3F\u3405\'3F\u3359\'3F\u3393\'3F \u3349\'3F\u3393\'3F\u3364\'3F\u3391\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F\u3374\'3F\u3405\'3F\u3370\'3F\u3403\'3F\u3380\'3F\u3393\'3F\u3330\'3F \u3342\'3F\u3368\'3F\u3405\'3F\u8205\'3F\u3377\'3F\u3398\'3F \u3374\'3F\u3368\'3F\u3384\'3F\u3405\'3F\u3384\'3F\u3405\'3F \u3370\'3F\u3391\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3403\'3F\u3359\'3F\u3405\'3F\u3359\'3F\u3405\'3F \u3370\'3F\u3403\'3F\u3381\'3F\u3393\'3F\u3349\'3F\u3375\'3F\u3390\'3F\u3375\'3F\u3391\'3F\u3376\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3393\'3F. \u3335\'3F\u3384\'3F\u3375\'3F\u3391\'3F\u3378\'3F\u3399\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3405\'3F. \u3333\'3F\u3381\'3F\u3379\'3F\u3393\'3F\u3374\'3F\u3402\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3393\'3F \u3356\'3F\u3392\'3F\u3381\'3F\u3391\'3F\u3354\'3F\u3405\'3F\u3354\'3F \u3376\'3F\u3363\'3F\u3405\'3F\u3359\'3F\u3393\'3F \u3381\'3F\u3452\'3F\u3383\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3398\'3F \u3347\'3F\u3452\'3F\u3374\'3F\u3405\'3F\u3374\'3F\u3349\'3F\u3379\'3F\u3391\'3F\u3378\'3F\u3399\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3405\'3F.}
\par\plain {\f0\fs24\b0 \loch\af0\hich\af0\dbch\af0\uc1\u3349\'3F\u3359\'3F\u3393\'3F\u3330\'3F \u3370\'3F\u3354\'3F\u3405\'3F\u3354\'3F \u3356\'3F\u3368\'3F\u3390\'3F\u3378\'3F\u3349\'3F\u3379\'3F\u3393\'3F\u3330\'3F, \u3354\'3F\u3393\'3F\u3381\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3354\'3F\u3393\'3F\u3381\'3F\u3376\'3F\u3393\'3F\u3349\'3F\u3379\'3F\u3393\'3F\u3330\'3F, \u3337\'3F\u3379\'3F\u3405\'3F\u3379\'3F\u3393\'3F \u3368\'3F\u3391\'3F\u3377\'3F\u3375\'3F\u3398\'3F \u3370\'3F\u3393\'3F\u3384\'3F\u3405\'3F\u3364\'3F\u3349\'3F\u3353\'3F\u3405\'3F\u3353\'3F\u3379\'3F\u3393\'3F\u3359\'3F\u3398\'3F \u3351\'3F\u3368\'3F\u3405\'3F\u3367\'3F\u3381\'3F\u3393\'3F\u3330\'3F \u3337\'3F\u3379\'3F\u3405\'3F\u3379\'3F \u3372\'3F\u3390\'3F\u3330\'3F\u3351\'3F\u3405\'3F\u3378\'3F\u3394\'3F\u3452\'3F \u3349\'3F\u3372\'3F\u3405\'3F\u3372\'3F\u3450\'3F \u3370\'3F\u3390\'3F\u3452\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3391\'3F\u3378\'3F\u3398\'3F \u3384\'3F\u3405\'3F\u3377\'3F\u3405\'3F\u3377\'3F\u3399\'3F\u3377\'3F\u3405\'3F\u3377\'3F\u3405\'3F \u3384\'3F\u3398\'3F\u3451\'3F\u3359\'3F\u3405\'3F\u3376\'3F\u3453\'3F \u3378\'3F\u3400\'3F\u3372\'3F\u3405\'3F\u3376\'3F\u3377\'3F\u3391\'3F\u3375\'3F\u3391\'3F\u3453\'3F \u3381\'3F\u3398\'3F\u3354\'3F\u3405\'3F\u3354\'3F\u3390\'3F\u3363\'3F\u3405\'3F \u3358\'3F\u3390\'3F\u3451\'3F \u3334\'3F\u3366\'3F\u3405\'3F\u3375\'3F\u3374\'3F\u3390\'3F\u3375\'3F\u3391\'3F \u3368\'3F\u3392\'3F\u3378\'3F \u3349\'3F\u3363\'3F\u3405\'3F\u3363\'3F\u3393\'3F\u3349\'3F\u3379\'3F\u3393\'3F\u3330\'3F, \u3349\'3F\u3377\'3F\u3393\'3F\u3364\'3F\u3405\'3F\u3364\'3F \u3349\'3F\u3403\'3F\u3378\'3F\u3451\'3F \u3374\'3F\u3393\'3F\u3359\'3F\u3391\'3F\u3375\'3F\u3393\'3F\u3330\'3F, \u3368\'3F\u3393\'3F\u3363\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3393\'3F\u3380\'3F\u3391\'3F\u3349\'3F\u3379\'3F\u3393\'3F\u3374\'3F\u3393\'3F\u3379\'3F\u3405\'3F\u3379\'3F \u3351\'3F\u3381\'3F\u3399\'3F\u3383\'3F\u3363\'3F \u3381\'3F\u3391\'3F\u3366\'3F\u3405\'3F\u3375\'3F\u3390\'3F\u3452\'3F\u3364\'3F\u3405\'3F\u3365\'3F\u3391\'3F\u3375\'3F\u3390\'3F\u3375\'3F \u3335\'3F\u3384\'3F\u3372\'3F\u3398\'3F\u3378\'3F\u3405\'3F\u3378\'3F\u3375\'3F\u3398\'3F \u3349\'3F\u3390\'3F\u3363\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F\u3364\'3F\u3405\'3F.}
\par\plain {\f0\fs24\b0 '\loch\af0\hich\af0\dbch\af0\uc1\u3342\'3F\u3368\'3F\u3405\'3F\u3364\'3F\u3390\'3F\u3363\'3F\u3405\'3F \u3368\'3F\u3391\'3F\u3368\'3F\u3405\'3F\u8205\'3F\u3377\'3F\u3398\'3F \u3370\'3F\u3399\'3F\u3376\'3F\u3391\'3F\u3368\'3F\u3405\'3F\u8205\'3F\u3377\'3F\u3398\'3F \u3333\'3F\u3452\'3F\u3365\'3F\u3330\'3F?' \u3346\'3F\u3376\'3F\u3391\'3F\u3349\'3F\u3405\'3F\u3349\'3F\u3453\'3F \u3384\'3F\u3393\'3F\u3378\'3F(*\u3346\'3F\u3376\'3F\u3393\'3F \u3335\'3F\u3368\'3F\u3405\'3F\u3364\'3F\u3405\'3F\u3375\'3F\u3451\'3F \u3381\'3F\u3400\'3F\u3451\'3F) \u3368\'3F\u3393\'3F\u3363\'3F\u3358\'3F\u3405\'3F\u3358\'3F\u3393\'3F \u3335\'3F\u3376\'3F\u3393\'3F\u3368\'3F\u3405\'3F\u3368\'3F \u3346\'3F\u3376\'3F\u3393\'3F \u3384\'3F\u3390\'3F\u3375\'3F\u3390\'3F\u3385\'3F\u3405\'3F\u3368\'3F\u3364\'3F\u3405\'3F\u3364\'3F\u3391\'3F\u3453\'3F \u3358\'3F\u3390\'3F\u3451\'3F \u3333\'3F\u3381\'3F\u3379\'3F\u3403\'3F\u3359\'3F\u3405\'3F \u3354\'3F\u3403\'3F\u3366\'3F\u3391\'3F\u3354\'3F\u3405\'3F\u3354\'3F\u3393\'3F.}
